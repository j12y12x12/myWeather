/* components/more_service.wxss */
.service-container {
  display: flex;
  flex-direction: column;
  margin: 0px 20px;
  margin-bottom: 30px;
}

.service-title {
font-size: 20px;
color: #111e36;
margin-bottom: 10px;
margin-top: 10px;

}
.buttons-content {
  display: flex;
  flex-direction: row;
  gap: 15px;

}

button {
  padding: 10px;
  font-size: 16px;
  border-radius: 5px;
  background-color: #dfdfdf;
  color: #111e36;
  width: auto;
  flex: 1;
}